<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.android.ads.manage.dao.IUsrBehaviorDao">
    <resultMap type="com.android.ads.manage.model.UsrBehavior" id="behavior">
		<id column="SEQ_ID" property="seqId" />
		<id column="USR_ONLYID" property="usrOnlyid" />
		<result column="BEHAVIOR_TYPE" property="behaviorType" />
		<result column="BEHAVIOR_SUBTYPE" property="behaviorSubType" />
		<result column="BEHAVIOR1" property="behavior1" />
		<result column="BEHAVIOR2" property="behavior2" />
		<result column="BEHAVIOR3" property="behavior3" />
		<result column="FLAG" property="flag" />
		<result column="CREATE_TIME" property="createTime" />
		<result column="UPDATE_TIME" property="updateTime" />
	</resultMap>
	
	<select id="queryUsrBehavior" parameterType="com.android.ads.manage.model.UsrBehavior" resultMap="behavior">    
        SELECT * FROM USR_BEHAVIOR limit 10;
	</select>
</mapper>